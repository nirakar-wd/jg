    <script>
        $(document).ready(function () {
            $("#tooglenight").change(function () {
                if (this.checked) {
                    $("#bd").addClass("night");
                    $(".content i").css("color", "#333");
                } else {
                    $("#bd").removeClass("night");
                    $(".content i").css("color", "white");
                }
            });
        });
    </script>

    <script src="/client/feedbacks.js">

    </script>

